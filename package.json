{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "d": "export BUILD_ENV=development && next dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npx eslint '**/*.{js,jsx}'",
    "lintfix": "npx eslint '**/*.{js,jsx}' --fix"
  },
  "dependencies": {
    "@moxy/next-layout": "^2.2.2",
    "@reduxjs/toolkit": "^1.5.0",
    "next": "10.0.3",
    "next-images": "^1.6.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "sass": "^1.30.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.11",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  }
}
